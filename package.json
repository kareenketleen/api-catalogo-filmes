{
  "name": "api-catalogo-filmes",
  "version": "1.0.0",
  "description": "API for a movie catalog",
  "main": "index.js",
  "scripts": {
	  "start": "ts-node-dev --ignore-watch node_modules src/server.ts",
    "dev": "ts-node-dev --ignore-watch node_modules src/server.ts",
    "knex:migrate:make": "knex --knexfile knexfile.ts migrate:make",
    "knex:migrate": "knex --knexfile knexfile.ts migrate:latest",
    "knex:seed": "knex --knexfile knexfile.ts seed:run"
  },
  "repository": "github.com/kareenketleen/api-catalogo-filmes",
  "author": "Kareen Castro",
  "license": "MIT",
  "dependencies": {
    "celebrate": "^15.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "knex": "^2.0.0",
    "multer": "^1.4.4",
    "sqlite3": "^5.0.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/hapi__joi": "^17.1.8",
    "@types/multer": "^1.4.7",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4"
  }
}
